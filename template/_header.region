<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{collection.navigationTitle|slugify}">

    <div class="page-loader">
      <div class="spinner"></div>
    </div>

    <div class="draw-the-line">

      <header class="main-header">
        <div class="inner-container clearfix">
          <a href="/">
            <div class="logo"></div> 
          </a> 
          <squarespace:navigation navigationId="mainNav" template="mainNav" />
        </div>
      </header>
